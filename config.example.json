{
  "stage1_output_dir": "./data",
  "stage2_output_dir": "./output",
  "remove_bytes_start": 100,
  "remove_bytes_end": 100,
  "code_config": {
    "github_access_key": "not-in-git",
    "max_repos": 1000,
    "languages_with_suffix": {
      "Ruby": ".rb",
      "C": ".c",
      "JavaScript": ".js"
    },
    "output_dir": "./data/code",
    "required_bytes": 1000000
  },
  "compiled_code_config": {
    "architectures": {
      "arm": "arm-linux-gnueabi-gcc -o OUTPUT_PATH INPUT_PATH",
      "mips": "mips-linux-gnu-gcc -EB -march=34kc -static INPUT_PATH -o OUTPUT_PATH",
      "win32": "i686-w64-mingw32-gcc INPUT_PATH -o OUTPUT_PATH.exe",
      "win64": "x86_64-w64-mingw32-gcc INPUT_PATH -o OUTPUT_PATH.exe"
    },
    "output_dir": "./data/compiled_code"
  },
  "images_config": {
    "google_api_key": "not-in-git",
    "google_cse_id": "not-in-git",
    "queries": [
      "nature",
      "cars"
    ],
    "formats": [
      "gif",
      "png"
    ],
    "output_dir": "./data/images",
    "required_bytes": 1100000
  },
  "text_config": {
    "langs": [
      "sk",
      "en",
      "pl"
    ],
    "queries": [
      "New York",
      "Barack Obama",
      "Edison"
    ],
    "formats": [
      "utf8",
      "utf16",
    ],
    "output_dir": "./data/text"
  }
}